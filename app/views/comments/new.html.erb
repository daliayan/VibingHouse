<%= form_for @comment, url: house_comments_path(@comment.house_id) do |f|%>
    <p style="font-size:18px; color:#0E1E02; margin-left: 20px"><%= f.label "Leave a comment here:" %></p>
    <p style="font-size:18px; color:#0E1E02; margin-left: 20px"><%= f.text_area :content, size: "30x4", required: true %></p>
    <p style="margin-left: 30px"><%= f.submit %></p>
<% end %>
<br>
